				<&clk_hdmi_sub>;
			clock-names = "clk_tcon_tv",
				"clk_hdmi",
				"clk_hdmi_24M",
				"rst_main";
			status = "okay";
			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				hdmi_in: port@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;
					hdmi_in_tcon2: endpoint@0 {
						reg = <0>;
						remote-endpoint = <&tcon2_out_hdmi>;
					};
				};
			};

