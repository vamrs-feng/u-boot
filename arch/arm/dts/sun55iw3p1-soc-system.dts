			clocks = <&clk_edp>, <&clk_edp_24m>;
			clock-names = "clk_edp", "clk_24m_edp";
			//FIXME
			status = "disabled";
			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				edp_in: port@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;
					edp_in_tcon3: endpoint@0 {
						reg = <0>;
						remote-endpoint = <&tcon3_out_edp>;
